<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.daemon.mobile.mobilemanager">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.daemon.mobile.application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/captain"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity
            android:name=".SettingActivity"
            android:label="@string/activity_setting_title"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".PhoneSafeActivity"></activity>
    </application>

</manifest>
